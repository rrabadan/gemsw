      <rParent name="muonBase:MUON" />
      <rChild name="mf:MEP" />
      <Translation x="0*cm" z="0*cm" y="0*cm" />
    </PosPart>
    <PosPart copyNumber="1">
      <rParent name="mf:MEP" />
      <rChild name="mf:GE0RingP" />
      <Translation x="0*cm" z="0*cm" y="110*cm" />
